  <course>
    <title>Spirituality and Social Work Practice </title>
    <courseCodes>
      <courseCode type="home listing">
        <subject>SOCW</subject>
        <courseNumber>481</courseNumber>
      </courseCode>
    </courseCodes>
    <gradingType>unrestricted</gradingType>
